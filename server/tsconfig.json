{
  "compilerOptions": {
    "jsx": "react",
    "target": "ES2022",
    "lib": [
      "ES2023",
      "DOM",
    ],
    "module": "Node16",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}