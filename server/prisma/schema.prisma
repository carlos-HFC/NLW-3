generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Orphanage {
  id             String   @id @default(uuid())
  name           String
  latitude       Decimal
  longitude      Decimal
  about          String
  instructions   String
  openingHours   String
  openOnWeekends Boolean  @default(false)
  createdAt      DateTime @default(now())
  updatedAt      DateTime @default(now()) @updatedAt
}
